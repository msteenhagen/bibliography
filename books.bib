@article{Cioffi2009-CIOMTU,
	Abstract = {The common assimilation of Wittgenstein\textquoteright{}s philosophical procedure to Freud\textquoteright{}s psychoanalytic method is a mistake. The concurrence of Freudian analysands is neither a necessary nor a sufficient condition of their unconscious thoughts having been detected. There are several sources of this error. One is the equivocal role Freud assign the patient\textquoteright{}s recognition of the correctness of his interpretation and in particular the part played by \textquoteleft{}paradoxical reminiscence\textquoteright: another, the surreptitious banalisation of Freud\textquoteright{}s procedure by followers---{}the reinvention of psychoanalysis as a phenomenological enterprise; still another, the appeal of the activity of giving fuller expression to one\textquoteright{}s tantalisingly vague and inexplicit thoughts and suspicions. This activity has its own intrinsic value though it ought not to be permitted to usurp the place of empirical investigation, as futile as this often is. And yet both plausible hypotheses and felicitous \textquoteleft{}further descriptions\textquoteright must yield in desirability to the attainment of a state of reconciliation to the person one has become however this was caused and whatever this is suspected to be.},
	Author = {Frank Cioffi},
	Date-Added = {2010-09-21 00:04:26 +0100},
	Date-Modified = {2010-09-21 00:07:13 +0100},

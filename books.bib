@article{dainton2010aa,
	Annote = {\S1 Intuitively, in our ordinary experience there is no discernible fusion or interpenetration of phnomenal qualities belonging to different sense-modes. The doctrine of `phnomenal interdependece' (PI) denies this. PI would have important implications for our understanding of consciousness. \S2 Sprigge, James and Bradley can be said to propose PI. Sprigge defends PI in its strongest form: For all states of consciousness it holds that every element is interdependent on the rest, as they lack identity independent of the total state. \S3 We admit that in \emph{some} cases this holds for \emph{some} features. The effects however seem contingent rather than necessary, and local rather than global (especially intermodal cases are pressing here). There appears no ground (yet) for the strong universal claim, and we might suppose Sprigge exagerrates. \S4 However, recent work in psychology and psychophysics indicates that intermodal dependencies abound. This nevertheless still does not establish Sprigge's universal position. And the dependencies might be of a different kind. Suppose $S$ at $t$ with state-parts P1... P$n$. These allow for deep and shallow interdependence, where the former claims that a P$n$ of $S$1 could not figure as such in $S$2---this is Sprigge's view. But considering what is going on in the illusion cases provides no evidence of teh deep interdepence view.\S5 We must admit, contrary to earlier claims, that some gestalts do provide indication for deep interdependece. One might argue that parts of aspects are interdependent by thair meaning. But the issue of high-level content in perception is controversial. And even if so, if would require all states to present us with a gestalt, and that's stretches the concept. \S6 Sprigge might argue from phenomenal unity itself---unity has an effect on all possible parts. This would be in effect an argument from co-consciousness, with an additional claim that parts aqcuire something extra in such a relation. This is plausible when we take it that experiences are identified by phenomenological character. Thus we have reason to require inclusive characterisation. If this is so, Sprigge's strong interdependence claim can be vindicated through the relation of co-consciousness.
},
	Author = {Barry Dainton},
	Date-Added = {2010-08-20 10:10:57 +0100},
	Date-Modified = {2010-08-20 18:20:19 +0100},
	Journal = {Royal Institute of Philosophy Supplement},
	Keywords = {Co-consciousness},
	Pages = {113-139},
	Scan/Read = {1},
	Title = {Phenomenal Holism},
	Volume = {67},
	Year = {2010}}

@incollection{heck2007aa,
	Annote = {Can the content of perception be nonconceptual and still enter into rational relations with conceptual states like belief? Fodor bring to bear empirical evidence that states of the visual system have nonconceptual content, but what is the significance of this, when correct? It is important to distinguish state views from content views. Proponents of content views understand conceptual contents as in some way coposed of concepts. In what way? And what are concepts? The suggestion defended is that the kind of content a state has depends on the kind of representation it is.

\S 1. There is a compelling explanatory reason for taking the contents of thought and belief to be conceptually structured. It explains, namely, how thoughts and beliefs are rationally, logically related to each other. There is a rationale for understanding the debate using a notion of `concept' that is non-trivial, in that it involves an ability to think of certain things a certain way.    

\S 2. We use cognitive maps as a distinct and efficient kind of representation---Fodor calls it iconic. These are not to be thought of as sturctures propositions, or possible worlds. They are geometric contents, in some sense that still requires psychological ellucidation.

\S 3. Perceptual contents do not straightforwardly satisfy the generality constraint. This also is an empirical matter, of course.

\S 4. We might think that the non-conceptual/conceptual distinction in the end is explained by differences in representation, yet there is reason to think that we commonly explain them in terms of differences in content. The proposal is that we regard the content of a mental state S as encoding not just what S represents but also how, that is, as encoding S's compositional structure.},
	Author = {Richard G Heck},
	Booktitle = {Contemporary Debates in Philosophy of Mind},
	Date-Added = {2010-08-20 10:04:09 +0100},
	Date-Modified = {2010-08-20 12:28:10 +0100},
	Editor = {Brian P. McLaughlin and Jonathan D. Cohen},
	Keywords = {Content, Nonconceptual Content},
	Pages = {117-138},
	Publisher = {Oxford: Blackwell Publishing},
	Scan/Read = {1},
	Title = {Are There Different Kinds of Content?},
	Year = {2007},

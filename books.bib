@article{baker2004aa,
	Annote = {Defends that artifacts have genuine ontological status: they exist as such (are substances), and not as things that merely fit a certain description that picks them out as artifacts. Defends her Contitution View, on which, if $x$ constitutes $y$, it are the features that make up $y$'s primary kind that determine the identity of $y$. In the case of artifacts, primary kindness is determined by proper function, where this proper function is a use or purpose intended by the author of the artifact. Provides conditions for $o$ being and artifact: $(A1)$ has maker(s) $M$, whose $(A2)$ intentions determine (in part) $o$'s primary kind, and $(A3)$ $o$'s existence depends on $M$'s intentions, and $(A4)$ $o$ is constituted by an aggregate constructed or selected by $M$. Also discusses Wiggins' 5 criteria for distinguishing artifacts from natural objects, and argues that 4 of these do not rule our artifacts as substances, while one would do so, but should be rejected as a criteria since it draws on an untennable mind-dependent/mind-independent (or nonnatural/natural) distinction.},
	Author = {Lynne Rudder Baker},
	Date-Added = {2011-01-14 12:24:53 +0000},
	Date-Modified = {2011-01-14 12:34:59 +0000},
	Issn = {1386-9795},
	Journal = {Philosophical explorations},
	Number = {2},
	Pages = {99-111},
	Publisher = {Routledge},
	Title = {The ontology of artifacts},
	Volume = {7},
	Year = {2004}}

@article{hilpinen1993aa,
	Annote = {Presents several constraints on artifacts and their (functional) properties. All artifacts have authors, and if $o$ is an artifact authored by $A$, there is a sortal description $K$ such that the existence of $o$ depends on $A$'s intention to produce an object of kind $K$, and $A$ accepts $o$ as a succesful realisation of the content of $A$'s intention. Introduces some further considerations about types and tokens, joint authorship, and identity of artifacts as dependent on intention instead of structural features.},
	Author = {Risto Hilpinen},

@book{austin1962aa,
	Annote = {Attacks modern sense-datum theories, and particularly their reliance on the Argument from Illusion (AI). Key target is Ayer. The core of AI is that we sometimes perceive sense data, when our perception deludes us (III). But `delusion' is not a straightforward concept (IV).  The follow-up step then is to argue that we always perceive sense data (V). Both steps are mistaken. Ayer recognises flaws, but accepts the most problematic aspects---he sees that it ultimately is a problem of the use of language (VI). Ayer sees two senses of `perceive' and 'see'. But there is no reason to take there to be different senses, Austin argues (IX); we can (a) just describe the things seen differently, or (b) see them in different ways. Moreover, the introduction of sense data is independent of all this. Ayer and Price present a technical sense of `perceive' as a solution to ambiguities, but it is just a means to get at incorrigible statements. Ayer wrongly takes there to be a class of incorrigible sentences, and another class of sentences that stand in need of evidence. But, Austin argues (X), this is based on neglecting the circumstances in which things are said, and it is furthermore misleading to think that statements about the material world `entail' statements about sense-data. There also is no reason to think that, globally, observation statements are precise, while material statements are vague. Warnock's reading of  and elaboration on Berkeley falls prey to the same objections (XI).},
	Author = {John L. Austin},
	Booktitle = {Sense and Sensibilia},
	Date-Added = {2010-06-13 12:33:27 +0100},
	Date-Modified = {2011-04-30 12:31:10 +0100},
	Editor = {G. J. Warnock},
	Publisher = {Oxford: Oxford University Press},
	Scan/Read = {1},
	Title = {Sense and Sensibilia},
	Year = {1962},
	Bdsk-File-1 = {}}

@incollection{wollheim2003aa,
	Annote = {Twofoldness marks experience of representational pictures. Such experience is seeing-in, and involves a perceptual skill. (1) Seeing-in is logically and historically prior to representation, and thus can define representational pictures. (2) Seeing-in can identify the representational content of a picture: a picture represents whatever can be correctly seen in it. Because pictures are artefacts, they imply such a standard of correctness. An independent and further point here is that correctness is determined by realised intentions of a maker.
If something cannot be seen in a painting, it cannot be part of its content. What are the limits of visibility?  Parts of the picture can be seen as anything as long as the concept of that thing is able to constrain and determine our perception. ``[I]n the domain of picture perception, perception exemplifies a rather remarkable degree of permeability by thought''(p.13). Seeing-in is one experience with two aspects. We should not explain the act of seeing-in by a prior experience, although we can refer to facts about the picture's surface that are non-experiential. },
	Author = {Richard Wollheim},
	Booktitle = {Looking into Pictures: An interdisciplinary approach to pictorial space},
	Crossref = {hecht03},
	Date-Added = {2010-06-13 12:14:31 +0100},
	Date-Modified = {2010-06-13 12:15:20 +0100},
	Pages = {3-15},
	Scan/Read = {1},
	Title = {In defence of seeing-in}}

@incollection{sedgwick2003aa,
	Annote = {Pictures can afford accurate perception of  specifiable (virtual) scenes. They make indirect perception of scenes, because of the intermediary of the representation which distinguishes it from ordinary (direct) perception. Pictures provide three kinds of information: (1) about the depicted, (2) about the surface, and (3) about the relation between surface and de viewers' surroundings. All three dimensions  allow for degrees of `virtual reality'. A parsimonious hypothesis has it that perceiving reality and virtual reality uses the same processing. Only if this hypothesis fails must we postulate perceptual mechanisms typical to indirect perception. It does not fail on a right understanding of direct perception.
\\
Direct perception has been understood classically through a `viewer centred' model. But this model is unsatisfactory, for it fails to account for distance. Three hypothesis are presented and discussed in relation to indirect perception. (a) Ground theory: attunement to complex spatial layouts of connected surfaces; (b) Environment-Centered Perception: attunement to geographical orientations of surfaces, objects on those surfaces, and sizes relative to the scale of the environment; and (c) Cross-Talk: dual awareness in direct perception of the three-dimensional scene and of its optical projection, and the perception of each is influenced by the other. This shows how we do not need any Pirenne-style compensation mechanisms specific to indirect perception.
},
	Author = {H. A. Sedgwick},
	Booktitle = {Looking into Pictures: An interdisciplinary approach to pictorial space},
	Crossref = {hecht03},
	Date-Added = {2010-06-13 12:11:38 +0100},
	Date-Modified = {2010-06-13 12:15:23 +0100},
	Pages = {61--75},
	Scan/Read = {1},
	Title = {Relating direct and indirect perception of spatial layout}}


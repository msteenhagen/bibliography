@article{Nudds:2001p6459,
author = {Matthew Nudds}, 
journal = {Croatian Journal of Philosophy},
title = {Common-Sense and Scientific Psychology},
abstract = {In this paper I discuss the circumstances in which it would be right to revise a common-sense psychological categorisation---such as the common-sense categorisa- tion of emotions---in the light of the results of empirical investigation. I argue that an answer to that question, familiar from eliminitivist arguments, should be rejected, and suggest that the issue turns on the ontological commitments of the explanations that common-sense psychological states enter into.},
affiliation = {Birkbeck College London},
annote = {Nudds argues that it is not straightforwardly clear that scientific understanding is able to force revision of commonsense psychology. His conclusion is that this is only possible if the theory which implicitly defines our common-sense psychological terms involves explanations which contain singular terms for events and things; they could be picked out by other disciplines, and hence other theories could show that commonsense explanations are false by showing that the events of things picked out in explanations don't have the properties we supposed them to have. 
On the other hand, if the theory which implicitly defines our common-sense psychological terms involves explanations which are sentential stative explanations then they will essentially involve relations between facts. The facts which make true the explanations of one theory cannot generally be identified with the facts which make true the explanations of a different theory, because in this case commonsense psychology would not be committed to the existence of explanatory particulars.

Nudds' main argument is in support of the claim that sentential stative explanation (where the causal claim asserts a relationship between facts, and these facts are states) is excluded from inter-theoretical identification. He shows that no strategy to reformulate causal claims of sentential stative explanation in terms of singular term explanation works ({\S}5). Therefore, sentential stative explanations have no ontological commitment to certain entities. But such ontological commitment is necessary for inter-theoretical identification ({\S}2). Thus there cannot be inter-theoretical identification. So, without inter-theoretical identification of terms, there is no possible way for forced revision by other theories.
},
number = {2},
pages = {171--180},
volume = {1},
year = {2001},
date-added = {2010-01-28 08:05:05 +0000},
date-modified = {2010-02-13 20:34:51 +0000},

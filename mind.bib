@article{Barker2003,
	author = {Barker, Stephen},
	title = {{Truth and Conventional Implicature}},
	journal = {Mind},
	year = {2003},
	volume = {112},
	number = {445},
	pages = {1-34},
	url = {http://mind.oxfordjournals.org/cgi/content/abstract/112/445/1},
	doi = {10.1093/mind/112.445.1},
	abstract = {Are all instances of the T-schema assertable? I argue that they are not. The reason is the presence of conventional implicature in a language. Conventional implicature is meant to be a component of the rule-based content that a sentence can have, but it makes no contribution to the sentence's truth-conditions. One might think that a conventional implicature is like a force operator. But it is not, since it can enter into the scope of logical operators. It follows that the semantic content of a sentence is not given simply by its truth-conditional content. So not all instances of the T-schema are assertable in the relevant sense. Consequently, there is a strong case to be made against truth-conditional semantics of the disquotational variety and deflationism about truth.},
	eprint = {http://mind.oxfordjournals.org/cgi/reprint/112/445/1.pdf}
}

